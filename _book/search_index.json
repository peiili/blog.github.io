 from \"clipboard\"\n\nexport default {\n  methods: {\n     copy() {\n      let clipboard = new Clipboard('.copy')\n      //复制成功\n      clipboard.on('success',e =>{\n        alert('已复制到剪切板')\n        clipboard.destroy()\n      })\n      //复制失败\n      clipboard.on('error',e =>{\n        alert('复制失败')\n        clipboard.destroy()\n      })\n    }\n  }\n}\n\n\n在传统HTML中使用\n\n\n\n    \n    \n    点击即复制\n    \n\n\n    \n\n\n    \n        \n    \n    \n\n\n\n\n    var clipboard = new ClipboardJS('.btn');\n\n    clipboard.on('success', function(e) {\n        alert('已将'+e.text+'复制到剪切板')\n        e.clearSelection();\n    });\n\n    clipboard.on('error', function(e) {\n        console.error('Action:', e.action);\n        console.error('Trigger:', e.trigger);\n    });\n\n\n插件中的事件:\nvar clipboard = new ClipboardJS('.btn');\n\nclipboard.on('success', function(e) {\n    console.info('Action:', e.action);\n    console.info('Text:', e.text);\n    console.info('Trigger:', e.trigger);\n\n    e.clearSelection();\n});\n\nclipboard.on('error', function(e) {\n    console.error('Action:', e.action);\n    console.error('Trigger:', e.trigger);\n});\nCopyright © peiili 2018 all right reserved，powered by Gitbook该文件修订时间：\n2018-11-04 15:14:52\n"},"page/service/php.html":{"url":"page/service/php.html","title":"服务端学习","keywords":"","body":"服务端学习服务端学习\nCopyright © peiili 2018 all right reserved，powered by Gitbook该文件修订时间：\n2018-10-06 01:42:28\n"},"page/service/nodeJS.html":{"url":"page/service/nodeJS.html","title":"nodeJs","keywords":"","body":"nodeJsnodeJs\nCopyright © peiili 2018 all right reserved，powered by Gitbook该文件修订时间：\n2018-10-06 01:42:28\n"},"page/design/":{"url":"page/design/","title":"设计学习","keywords":"","body":"设计学习设计学习\nCopyright © peiili 2018 all right reserved，powered by Gitbook该文件修订时间：\n2018-10-06 01:38:45\n"},"page/design/xingse.html":{"url":"page/design/xingse.html","title":"形 色","keywords":"","body":"形 色形 色\nCopyright © peiili 2018 all right reserved，powered by Gitbook该文件修订时间：\n20